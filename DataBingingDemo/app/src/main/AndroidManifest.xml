<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.axon.databingingdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ui.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ui.EventHanderActivity" />
        <activity android:name=".ui.BaseUserActivity" />
        <activity android:name=".ui.DoubleBindingActivity" />
        <activity android:name=".ui.ObservableActivity" />
        <activity android:name=".ui.LayoutDetailActivity" />
        <activity android:name=".ui.RecyclerViewActivity" />
        <activity android:name=".ui.MultiRecyclerViewActivity" />
        <activity android:name=".ui.CustomAttributeActivity"/>
        <activity android:name=".ui.ViewStubActivity" />
        <activity android:name=".ui.ViewModelActivity" />
        <activity android:name=".ui.NameActivity" />
    </application>

</manifest>